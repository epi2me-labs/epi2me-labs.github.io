rototype.propertyIsEnumerable.call(source, key)) continue; target[key] = source[key]; } } return target; }\n\nfunction _objectWithoutPropertiesLoose(source, excluded) { if (source == null) return {}; var target = {}; var sourceKeys = Object.keys(source); var key, i; for (i = 0; i < sourceKeys.length; i++) { key = sourceKeys[i]; if (excluded.indexOf(key) >= 0) continue; target[key] = source[key]; } return target; }\n\n/* @jsxRuntime classic */\n\n/* @jsx mdx */\nvar _frontmatter = {\n  \"title\": \"Normalizing fastq data\",\n  \"author\": \"Chris Wright\",\n  \"slug\": \"introducing-fastcat\",\n  \"category\": \"Articles\",\n  \"date\": \"2021-07-07T00:00:00.000Z\",\n  \"tags\": [\"#fastq\", \"#tools\"],\n  \"keywords\": [\"epi2melabs\", \"nanopore sequencing\", \"fastq\", \"QC\"],\n  \"description\": \"An introduction to fastcat -- the program we use in our workflows for manipulating fastq files from Nanopore sequencing devices.\"\n};\n\nvar makeShortcode = function makeShortcode(name) {\n  return function MDXDefaultShortcode(props) {\n    console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\");\n    return mdx(\"div\", props);\n  };\n};\n\nvar Bokeh = makeShortcode(\"Bokeh\");\nvar layoutProps = {\n  _frontmatter: _frontmatter\n};\nvar MDXLayout = \"wrapper\";\nreturn function MDXContent(_ref) {\n  var components = _ref.components,\n      props = _objectWithoutProperties(_ref, _excluded);\n\n  return mdx(MDXLayout, _extends({}, layoutProps, props, {\n    components: components,\n    mdxType: \"MDXLayout\"\n  }), mdx(\"p\", null, \"Oxford Nanopore Sequencing devices output RNA and DNA read sequences in batched\\n\", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"https://en.wikipedia.org/wiki/FASTQ_format\"\n  }, \"fastq\"), \" files. This is useful for\\nreal-time analyses like those run by EPI2ME but can be inconvenient for providing\\nthe data to standard bioinformatics tools, many of which only cater for data\\nprovided in a single file. In this post we\\u2019ll examine a new tool developed for\\nuse in EPI2ME Labs workflows to provide a first-line data sanitization and\\nnormalization step.\"), mdx(\"h2\", {\n    \"id\": \"the-problem\"\n  }, \"The problem\"), mdx(\"p\", null, \"The fastq format is ubiquitous for storing sequencing reads. Data to be analysed\\nby a workflow can however be presented with at least three wrinkles:\"), mdx(\"ol\", null, mdx(\"li\", {\n    parentName: \"ol\"\n  }, \"there might be multiple fastq files to be analysed as a whole,\"), mdx(\"li\", {\n    parentName: \"ol\"\n  }, \"the files may contain reads from a multiplexed experiment to be analysed separately, \"), mdx(\"li\", {\n    parentName: \"ol\"\n  }, \"or the files may or may not be compressed with \", mdx(\"a\", {\n    parentName: \"li\",\n    \"href\": \"https://en.wikipedia.org/wiki/Gzip\"\n  }, \"gzip\"), \" compression.\")), mdx(\"p\", null, \"We would like a process to handle these situations to format the data in a\\nstandardised form: a single gzip compressed fastq file for each demultiplexed\\nsample. There are already a variety of programs that can take\\nas input the data described above and get us most of the way to the desired\\nresult. Indeed for most cases simple shell scripting can do the job. The standard\\nLinux \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"cat\"), \" program can be used in simple cases with either uncompressed\\nor gzip compressed data, but not a mixture. The demultiplexing aspect of the\\ntask complicates things further: Oxford Nanopore Sequencing devices output\\ndemultiplexed reads helpfully into distinct directories, but what if this\\nstructure has been lost? We would like a more robust solution.\"), mdx(\"h2\", {\n    \"id\": \"the-solution\"\n  }, \"The solution\"), mdx(\"p\", null, \"To solve this problem the EPI2ME Labs team developed the \", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"https://github.com/epi2me-labs/fastcat\"\n  }, \"fastcat\"), \"\\nprogram. The program handles the three concerns above placing minimal contraints\\non the inputs. It performs this task using two key ingredients:\"), mdx(\"ol\", null, mdx(\"li\", {\n    parentName: \"ol\"\n  }, \"the stream-based fastq/fasta parser \", mdx(\"a\", {\n    parentName: \"li\",\n    \"href\": \"https://github.com/attractivechaos/klib\"\n  }, \"kseq\"), \" as used in\\nthe ubiquitous \", mdx(\"a\", {\n    parentName: \"li\",\n    \"href\": \"https://github.com/samtools/htslib\"\n  }, \"htslib\"), \" package,\"), mdx(\"li\", {\n    parentName: \"ol\"\n  }, \"the header information written to fastq records by the Guppy basecaller.\")), mdx(\"p\", null, \"The program is not yet a complete replacement to more fully\\nfeatured fastq toolkits such as the excellent \", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"https://bioinf.shenwei.me/seqkit/\"\n  }, \"seqkit\"), \" though\\ndoes provide a few additional generally useful features in addition to solving\\nthe core normalization functions. These include length and quality filtering, as\\nwell as producing a summary file containing per-read statistics. Being written\\nin C it performs these tasks \", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"https://github.com/lh3/biofast/tree/master/fqcnt\"\n  }, \"several times faster\"), \"\\nthan any similar programs written in languages such as Python.\"), mdx(Bokeh, {\n    plotJson: yieldPlot,\n    plotName: \"yield-plot\",\n    plotCaption: \"The fastq header parsing of fastcat allows plotting experimental yield withou resorting to Fast5 or standalone sequencing summary files.\",\n    mdxType: \"Bokeh\"\n  }), mdx(\"h2\", {\n    \"id\": \"summary\"\n  }, \"Summary\"), mdx(\"p\", null, \"Bioinformatics workflows often require using tools which are not designed\\nto work in a real-time, streaming fashion and require all sequencing data\\nto be provided in a single input file. fastcat provides a convenient method\\nto normalize input data for downstream tools.\"), mdx(\"p\", null, mdx(\"strong\", {\n    parentName: \"p\"\n  }, \"Key fastcat features\")), mdx(\"ul\", null, mdx(\"li\", {\n    parentName: \"ul\"\n  }, \"accepts compressed, uncompressed, or a mixture of fastq files,\"), mdx(\"li\", {\n    parentName: \"ul\"\n  }, \"filters reads on length and quality score,\"), mdx(\"li\", {\n    parentName: \"ul\"\n  }, \"produces per-read and per-input file summaries including information from fastq headers,\"), mdx(\"li\", {\n    parentName: \"ul\"\n  }, \"recapitulate standard MinKNOW demultiplexing directory layout from unstructured data. \")));\n}\n;\nMDXContent.isMDXComponent = true;","keywords":["epi2melabs","nanopore sequencing","fastq","QC"],"tags":[{"id":"6815e371-f656-5eb6-8bf1-16ae2d9ac5b0","name":"#fastq","slug":"/tag/fastq/"},{"id":"996e6a9a-688a-5803-bba0-e92efa02725d","name":"#tools","slug":"/tag/tools/"}],"thumbnail":null},"tagCategoryPosts":{"nodes":[]},"tagPosts":{"nodes":[]},"categoryPosts":{"nodes":[{"id":"5fe063bf-eb68-5e06-8e7f-a434e3cac57d","title":"Two years with Nextflow","slug":"/two-years-of-nextflow/","link":null,"excerpt":"The EPI2ME Labs team has now been using  Nextflow  for\nalmost two years. In that time we have built…","timeToRead":11,"featured":null,"thumbnailText":null,"date":"August 27, 2022","category":{"id":"70ca2f81-42ba-5850-a56a-6f79881049b0","name":"Articles","slug":"/category/articles/","color":"#A53F96","icon":"/static/a0eadc6b0bbda21a5e4290df3c07d3b4/articles.svg"},"author":{"id":"9c333548-4948-5566-a24c-cc1cc8545dcc","name":"Chris Wright","slug":"/author/chris-wright/","title":"Director Customer Workflows","description":"I co-ordinate the activities of the Customer Workflows Group at Oxford Nanopore Technologies. Previously I worked in the Research division at ONT leading the Scientific Data Analysis Group.","skills":["Scientific software design","Computational biology","Consensus and variant calling"],"social":[{"name":"Twi